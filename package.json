{
  "name": "@sigmaott/nestjs-ksqldb-client",
  "version": "1.0.1",
  "description": "ksqldb-client for nestjs",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "test": "jest --forceExit",
    "lint-staged": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:SigmaOTT/nestjs-ksqldb-client.git"
  },
  "keywords": [
    "nestjs",
    "ksqldb-client"
  ],
  "author": "Luu Viet Anh",
  "license": "MIT",
  "homepage": "https://github.com/SigmaOTT/nestjs-ksqldb-client#readme",
  "dependencies": {
    "@golevelup/nestjs-modules": "^0.4.3",
    "ksqldb-client": "^1.0.5"
  },
  "devDependencies": {
    "@nestjs/common": "8.4.0",
    "@nestjs/core": "^8.4.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nestjs/testing": "8.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
